{
  "name": "kinesis-consumer-livestream",
  "version": "2.0.0",
  "description": "Kinesis consumer for the ingestion api. Logs events to a file to be picked up by Filebeat.",
  "main": "lib/index.js",
  "scripts": {
    "dev": "kcl-bootstrap -e -p properties/dev.properties -j /usr/bin/java",
    "start": "kcl-bootstrap -e -p properties/prod.properties -j /usr/bin/java",
    "tail": "tail -f app.log | bunyan -o short",
    "test": "mocha -r babel-register -r babel-polyfill -r dotenv/config test/index.test.js"
  },
  "author": "Alois <alois@astronomer.io>",
  "license": "MIT",
  "dependencies": {
    "@astronomer/kinesis-consumer": "^6.0.0",
    "aws-kcl": "^0.6.0",
    "bunyan": "^1.8.10",
    "dotenv": "^4.0.0",
    "streamroller": "^0.4.1",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "chai": "^4.0.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-import": "^0.13.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "mocha": "^3.4.2",
    "sinon": "^2.3.2"
  }
}
